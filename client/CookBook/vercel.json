{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "package.json"
      }
    ],
    "rewrites":  [
        {"source": "/(.*)", "destination": "/"}
      ]
}